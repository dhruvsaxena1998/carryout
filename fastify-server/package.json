{
  "name": "fastify-server",
  "version": "1.0.0",
  "description": "typescript fastify server",
  "main": "app.ts",
  "directories": {
    "src": "src"
  },
  "scripts": {
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\"  \"tsc -w\" \"fastify start -w -l info -P dist/app.js\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.3.1",
    "fastify-cli": "^2.10.0",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.0",
    "mysql2": "^2.2.5",
    "qs": "^6.10.1",
    "sequelize": "^6.6.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "@types/qs": "^6.9.6",
    "concurrently": "^6.0.0",
    "fastify-tsconfig": "^1.0.1",
    "typescript": "^4.1.3"
  }
}
